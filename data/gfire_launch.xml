<?xml version='1.0' encoding='UTF-8' ?>
<!--
* This XML file is what gfire plugin for pidgin uses
* to determine which games you have that can be
* joined, and how exactly to launch them. The file is
* generated by the game manager and is just an example.
-->

<launchinfo>
	<game id='4097' name='Wolfenstein: Enemy Territory' type='Native game'>
		<xqf name=''/>
		<processes unix_process='et.x86' windows_process='et.x86'/>
		<command>
			<bin>et.x86</bin>
			<dir>/usr/local/games/enemy-territory</dir>
			<gamemod/>
			<connect>+connect @ip@:@port@</connect>
			<launch>@options@ @gamemod@ @connect@</launch>
		</command>
	</game>
	<game id='5080' name='Enemy Territory: QUAKE Wars' type='Native game'>
		<xqf name=''/>
		<processes unix_process='etqw.x86' windows_process=''/>
		<command>
			<bin>etqw</bin>
			<dir>/home/bin</dir>
			<gamemod/>
			<connect>+connect @ip@:@port@</connect>
			<launch>@options@ @gamemod@ @connect@</launch>
		</command>
	</game>
	<game id='4920' name='Defcon' type='Native game'>
		<xqf name=''/>
		<processes unix_process='defcon.bin.x86' windows_process=''/>
		<command>
			<bin>bin</bin>
			<dir>/home/laurent</dir>
			<gamemod></gamemod>
			<connect>+connect @ip@:@port@</connect>
			<launch>@options@ @gamemod@ @connect@</launch>
		</command>
	</game>
</launchinfo>
